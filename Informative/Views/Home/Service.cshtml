
@{
    ViewBag.Title = "Overhead Door";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" href="../Content/images/icons/favicon.ico" />
    <title>@ViewBag.Title</title>
    <link href="../Content/fonts/oswald/stylesheet.css" rel="stylesheet" />
    <link href="../Content/css/bootstrap.min.css" rel="stylesheet">
    <link href="../Content/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <!-- Custom styles for this template -->
    <link href="../Content/css/style.css" rel="stylesheet">
    <link href="../Content/css/custom.css" rel="stylesheet">
    <link href="../Content/css/responsive.css" rel="stylesheet">
    <script src="../Scripts/js/jquery.min.js" type="text/javascript"></script>
    <script src="../Scripts/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="../Scripts/js/custom.js" type="text/javascript"></script>
</head>
<body class="about">


    <!--header--->
    <header>
        <div class="container">
            <div class="header-top">
                <div class="row">
                    <div class="col-md-6 col-sm-6 col-xs-6">
                        <p>3741 Grandview Road Millbrook, AL 36054</p>
                    </div>
                    <div class="col-md-6 col-sm-6 col-xs-6 text-right">
                        <p>Monday through Friday: 8:00 am - 4:00 pm</p>
                    </div>
                </div>
            </div>
        </div>
        <nav class="navbar navbar-default">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12 visible-xs">
                        <div class="logo-mobile">
                            <a href="#"><img src="../Content/images/Home/5.jpg" class="img-responsive" /></a>
                        </div>
                    </div>
                    <div class="col-md-11 col-xs-12">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        </div>
                        <div class="collapse navbar-collapse text-center" id="myNavbar">
                            <ul class="nav navbar-nav margin-l">
                                <li><a href="#">Home</a></li>
                                <li class="active"><a href="#">Contact</a></li>
                                <li><a href="#">Support</a></li>
                                <li><a href="#">Repair & Service</a></li>
                            </ul>
                            <ul class="nav navbar-nav navbar-right">
                                <li><a href="#">Residential</a></li>
                                <li><a href="#">Commercial</a></li>
                                <li><a href="#">Fireplace & Logs</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-1">
                    </div>
                </div>
            </div>
        </nav>
        <div class="clearfix"></div>
    </header>
    <!--end-->
    <section class="welcome-block">
        <div class="container">
            @*<div class="height-135"></div>*@
            <div class="image hidden-xs">
                <a><img class="img-responsive" src="../Content/images/Home/5.jpg" alt="image"></a>
            </div>
        </div>
        <div class="clearfix"></div>
    </section>




    <!--footer-->
    <footer>
        <div class="container">
            <div class="row footer-block-main text-center">
                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 width-50">
                    <div class="footer-block">
                        <h4>quick links</h4>
                        <ul class="list-unstyled">
                            <li><a href="@Url.Action("Index", "Home")">Home</a> | <a href="#">Residential</a></li>
                            <li><a href="#">Contact</a> | <a href="#">Commercial</a></li>
                            <li><a href="#">Support</a> | <a href="#">Fireplace & Logs</a></li>
                            <li><a href="#">Repair & Service</a>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 width-50">
                    <div class="footer-block">
                        <h4>contact</h4>
                        <ul class="list-unstyled">
                            <li><p>week days : 800 AM - 400 PM</p></li>
                            <li><p>334-285-5275</p></li>
                            <li><p>info@email.com</p></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 width-50">
                    <div class="footer-block">
                        <h4>address</h4>
                        <ul class="list-unstyled">
                            <li>3741 Grandview Road</li>
                            <li>Millbrook, AL</li>
                            <li>36054</li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-10 col-md-offset-1">
                    <hr />
                </div>
                <div class="clearfix"></div>
                <div class="col-md-12">
                    <div class="social-grid text-right">
                        <ul>
                            <li><a><i class="fa fa-facebook"></i></a></li>
                            <li><a><i class="fa fa-twitter"></i></a></li>
                            <li><a><i class="fa fa-instagram"></i></a></li>
                            <li><a><i class="fa fa-google-plus"></i></a></li>
                            <li><a><i class="fa fa-youtube-play"></i></a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="copyright">
                        © 2018 All rights reserved <br />
                        Overhead Door Company of Montgomery <br />
                        Designed and hosted by <a href="https://www.codeheight.com/" target="_blank">CodeHeight Solutions, LLC</a>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!--end-->
    <!--back to top--->
    <a id="back-to-top" class="scrollTop back-to-top" href="javascript:void(0);" style="display: none;">
        <img src="../Content/images/top-arrow.png" onmouseover="this.src = '../Content/images/top-arrow2.png'" onmouseout=" this.src='../Content/images/top-arrow.png' " alt="left">
    </a>
</body>
</html>